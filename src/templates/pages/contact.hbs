<head>

    {{> common/head }}

</head>

<body>

    {{> common/header }}
    <section id="contact">
        <div class="map overflow-hidden">
            <iframe
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d10832.859622851576!2d49.811494355104436!3d40.367440229944805!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40307d92aa9a9515%3A0xcf64085f7a00a7be!2sVMF%20Kontinental!5e0!3m2!1sru!2s!4v1625924525035!5m2!1sru!2s"
                width="140%" height="500" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
        </div>
        <div class="contact-info">
            <div class="container">
                <div class="row">
                    <div class="col-md-6 col-12">
                        <div class="contact-form">
                            <p class="form-title">Əlaqə formu</p>
                            <form>
                                <div class="form-group">
                                    <label for="full-name">Tam adınız <span>*</span></label>
                                    <input type="email" class="form-control" id="full-name" placeholder="Daxil edin">

                                </div>
                                <div class="form-group">
                                    <label for="email">E-mail <span>*</span></label>
                                    <input type="text" class="form-control" id="email" placeholder="Daxil edin">
                                </div>
                                <div class="form-group">
                                    <label for="message">İsmarıc <span>*</span></label>
                                    <textarea class="form-control" name="message" id="message" cols="30" rows="6"
                                        placeholder="Message..."></textarea>
                                </div>

                                <button type="submit" class="btn proff-btn">Göndər</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-md-6 col-12 mt-4">
                        <div class="row">
                            <div class="col-6 ">
                                <p class="contact-title ml-5">
                                    PROFF KİTCHEN
                                </p>
                                <ul class="list-inline contact-list ml-5">
                                    <li class="list-inline-item ">
                                        P.O: AZ 1014
                                    </li>
                                    <li class="list-inline-item ">
                                        A.Qaibov küçəsi 10h
                                    </li>
                                    <li class="list-inline-item ">
                                        Baku, Azerbaijan
                                    </li>
                                </ul>
                            </div>
                            <div class="col-6">
                                <p class="contact-title">
                                    INGRESS CONTACTS
                                </p>
                                <ul class="list-inline contact-list">
                                    <li class="list-inline-item ">
                                        info@proffkitchen.az</li>
                                    <li class="list-inline-item">
                                        +994 50 8021020</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    {{> bests }}
    {{> common/footer }}

</body>

</html>