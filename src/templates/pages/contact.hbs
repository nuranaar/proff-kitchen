<head>

    {{> common/head }}

</head>

<body>

    {{> common/header }}
    <section id="contact">
        <div class="map overflow-hidden">
            <div id="map" style="width:140%; height:550px"></div>
        </div>
        <div class="contact-info">
            <div class="container">
                <div class="row">
                    <div class="col-md-6 col-12">
                        <div class="contact-form">
                            <p class="form-title">Əlaqə formu</p>
                            <form id="contact-form">
                                <div class="form-group">
                                    <label for="full-name">Tam adınız <span>*</span></label>
                                    <input type="text" class="form-control" id="full-name" name="full-name"
                                        required="true" placeholder="Daxil edin">

                                </div>
                                <div class="form-group">
                                    <label for="email">E-mail <span>*</span></label>
                                    <input type="email" class="form-control" id="email" name="email" required="true"
                                        placeholder="Daxil edin">
                                </div>
                                <div class="form-group">
                                    <label for="message">İsmarıc <span>*</span></label>
                                    <textarea class="form-control" name="message" id="message" cols="30" rows="6"
                                        required="true" maxlength="160" placeholder="Message..."></textarea>
                                    <div class="text-length">
                                        0/160
                                    </div>
                                </div>

                                <button type="submit" class="btn proff-btn">Göndər</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-md-6 col-12 ">
                        <div class="row ml-5">
                            <div class="col-lg-6 col-12 mt-4">
                                <p class="contact-title ">
                                    PROFF KİTCHEN
                                </p>
                                <ul class="list-inline contact-list">
                                    <li class="list-inline-item ">
                                        P.O: AZ 1014
                                    </li>
                                    <li class="list-inline-item ">
                                        A.Qaibov küçəsi 10h
                                    </li>
                                    <li class="list-inline-item ">
                                        Baku, Azerbaijan
                                    </li>
                                </ul>
                            </div>
                            <div class="col-lg-6 col-12 mt-4">
                                <p class="contact-title">
                                    INGRESS CONTACTS
                                </p>
                                <ul class="list-inline contact-list">
                                    <li class="list-inline-item ">
                                        info@proffkitchen.az</li>
                                    <li class="list-inline-item">
                                        +994 50 8021020</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    {{> bests }}
    {{> common/footer }}

    <script>
        function initMap() {
            // The location of Uluru
            const uluru = { lat: 40.3762987, lng: 49.8203356 };
            // The map, centered at Uluru
            const map = new google.maps.Map(document.getElementById("map"), {
                mapId: '5ecd3b5bf978a98f',
                zoom: 17,
                center: uluru,
            });
            // The marker, positioned at Uluru
            const marker = new google.maps.Marker({
                position: uluru,
                map: map,
            });
        }
    </script>
    <script
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBOLgEVxHjFa7o1b8iIvkzOuxqinHAiaLc&callback=initMap&libraries=&v=weekly"
        async></script>
</body>

</html>